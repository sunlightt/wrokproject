page {
    background: #eee;
}
.info-box {
    width: 96%;
    margin: 0 auto;
    text-align: center;
    background: #fff;
    padding: 30rpx 0;
}

.info-box image {
    width: 100rpx;
    height: 100rpx;
    border-radius: 50%;
    margin-bottom: 18rpx;
}

.name {
    font-size: 30rpx;
    text-align: center;
    color:#333;
}

.list-box {
    width: 96%;
    margin: 0 auto;
    margin-top: 50rpx;
}

.list {
    font-size: 30rpx;
    background: #fff;
}

.text-wrap {
    margin-left: 40rpx;
    border-bottom: 1px solid #eee;
    height: 100rpx;
    line-height: 100rpx;
    padding-right: 40rpx;
}

.list text:nth-child(1) {
    float: left;
}

.list text:nth-child(2) {
    float: right;
}

.title {
    margin-top: 20rpx;
}

.title view {
    margin-top: 14rpx;
    text-align: center;
    font-size: 24rpx;
}

.btn-box {
    height: 80rpx;
    line-height: 80rpx;
    margin-top: 40rpx;
}

.agree {
    margin: 0 auto;
}

.agree>view, .refuse>view {
    width: 360rpx;
    height: 70rpx;
    line-height: 74rpx;
    text-align: center;
    font-size: 30rpx;
    color: #fff;
    margin: 0 auto;
}

.agree view {
    background: #8fc320;
}
