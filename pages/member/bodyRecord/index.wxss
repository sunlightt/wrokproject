page{
  padding: 40rpx 12rpx;
  box-sizing: border-box;
}
.list-time{
  background: url('/resources/img/bodyBg.png') repeat;
  background-size:100%;
  height: 800rpx;
  padding-top:40rpx;
  box-sizing: border-box;
  box-shadow: -3px -3px 6px rgba(30,30,30,0.1);
  position: relative;
  margin-bottom: 100rpx;
}
.yellow-body{
  width:26%;
  height: 400rpx;
  position: absolute;
  top:50%;
  left:50%;
  transform: translate(-50%,-50%);
}
.number-box{
  display: flex;
  height: 200rpx;
}
.body-number{
  flex:1;
}
.body-number view{
  text-align: center;
  width: 100%;
}
.record-number{
  font-size: 60rpx;
}
.record-type{
  margin-top:16rpx;
  color:#969696;
  font-size: 24rpx;
}
.report-time{
  height: 46rpx;
  line-height: 46rpx;
  background: #000;
  color:#ffff00;
  padding: 0rpx 36rpx;
  font-size: 31rpx;
  position: absolute;
  top:-23rpx;
  right: 20rpx;
  border-radius: 10rpx;
}