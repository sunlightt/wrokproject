.list{
  position: relative;
  padding-left: 160rpx;
  height: 200rpx;
  box-shadow: 0rpx -2rpx 16rpx rgba(33,33,33,0.2);
  margin-bottom: 40rpx;
}
.list-left{
  width: 160rpx;
  height: 200rpx;
  position: absolute;
  left: 0rpx;
  top: 0rpx;
  text-align: center;
}
.list-right{
  height: 200rpx;
}
.list-right view{
  height: 70rpx;
  line-height: 120rpx;
}
.order-number{
  font-size: 80rpx;
  color: #333;
  height: 117rpx;
  line-height: 130rpx;
}
.time{
  font-size: 26rpx;
  color:#959595;
  height: 50rpx;
  line-height: 50rpx;
}
.type{
  float: right;
  color:#959595;
  font-weight: 600;
  font-size: 28rpx;
  height: 100%;
  margin-right: 12rpx;
  line-height:70px; 
}
.content{
  font-size: 30rpx;
  font-weight: 600;
}