page{
  background: #eee;
  padding-top: 30rpx;
  box-sizing: border-box;
}
.list-box{
  background: #fff;
  width: 96%;
  margin: 0 auto;
  padding-bottom: 50rpx;
  padding-top:30rpx;
}
.list{
  padding-left:120rpx;
  box-sizing: border-box;
  height: 100rpx;
  line-height: 100rpx;
  position: relative;
  padding-right: 60rpx;
}
.head-figure{
  height: 100rpx;
  width: 120rpx;
  text-align: center;
  position: absolute;
  left: 0rpx;
  top: 0rpx;
}
.head-figure image{
  width:80rpx;
  height: 80rpx;
  border-radius: 50%;
  vertical-align: middle;
  margin-top: -6rpx;
}
.name{
  width: 100%;
  height:100rpx;
  border-bottom:1px solid #eee;
  font-size: 30rpx;
  line-height: 100rpx;
  color:#617D89;
  margin-left: 10rpx;
  text-align:left;
}
.right{
  text-align: center;
  width:60rpx;
  height: 100rpx;
  line-height: 100rpx;
  position: absolute;
  right: 0rpx;
  top: 0rpx;
  font-weight: 600;
}
