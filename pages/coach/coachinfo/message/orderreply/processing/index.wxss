/* processing.wxss */
page{
  padding:20rpx 10rpx;
  background: #E4E4E4;
  box-sizing:border-box;
}
.list{
  font-size: 30rpx;
  background: #fff;
}


.text-wrap{
  margin-left:40rpx;
  border-bottom:1px solid #eee;
  height:100rpx;
  line-height: 100rpx;
  padding-right:40rpx;  
}
.list text:nth-child(1){
  float: left;
}
.list text:nth-child(2){
  float:right; 
}
.healthy-food{
  background: #fff;
  height: 140rpx;
  padding:30rpx 0;
  font-size:30rpx;
}
.title{
  height: 60rpx;
  line-height: 60rpx;
  margin-bottom: 20rpx;
  margin-left: 30rpx;
}
.first-radio{
  margin-right: 80rpx;
  margin-left: 40rpx;
}
.save-btn{
  width:90%;
  margin:0 auto;
  height: 70rpx;
  line-height: 70rpx;
  font-size: 32rpx;
  color:#fff;
  background: #8FC320;
  text-align: center;
  margin-top: 50rpx;
  border-radius: 8rpx;
}
.complete{
  color:#8BC34A;
}