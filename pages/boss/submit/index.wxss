
.parent{
     width:75%;
     margin:0 auto;
     padding-top:10%;
}
.title{
    text-align:center;
    margin-bottom:30rpx;
}
.title text{
    display:inline-block;
    font:40rpx/60rpx '微软雅黑';
    font-weight:bold;
    color:#333;
    padding-right:50rpx;
    background:url('http://o6nl1koop.bkt.clouddn.com/subicon.png') no-repeat right center;
    background-size:15%;
}

.inf{
    display:block;
    text-align:center;
    font:30rpx/50rpx '微软雅黑';
    color:#333;
}
input{
    width:100%;
    height:70rpx;
    border:1rpx solid #333;
    font:30rpx/80rpx '微软雅黑'；
    color：#333;
    box-sizing:border-box;
    margin:30rpx 0;
    padding:0 10rpx;
}

.upimg{
    display:flex;
    flex-direction:row;
}
.upimg .img{
    width:220rpx;
    height:220rpx;
    box-sizing:border-box;
    border:1rpx solid #333;
    padding:10rpx;
    margin-right:40rpx;
}
.upimg .img image{
    display:block;
    width:100%;
    height:100%;
}
.upimg .msg{
    display:flex;
    flex-direction:column;
    justify-content:center;
}
.upimg .msg text{
    font:30rpx/50rpx '微软雅黑';
    color:#333;
}

button{
     width:408rpx;
     font:30rpx/80rpx '微软雅黑';
     color:#333;
     margin:80rpx auto 0;
     background:#fff100;
}