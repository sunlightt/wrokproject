
.parent{
     width:75%;
     margin:0 auto;
     padding-top:10%;
}
.title{
    text-align:center;
    margin-bottom:30rpx;
}
.title text{
    display:inline-block;
    font:40rpx/60rpx '微软雅黑';
    font-weight:bold;
    color:#333;
    padding-right:50rpx;
    background:url('http://o6nl1koop.bkt.clouddn.com/subicon.png') no-repeat right center;
    background-size:10%;
}
.img{
    width:454rpx;
    height:454rpx;
    padding:20rpx;
    border:1rpx solid #333;
    box-sizing:border-box;
    margin:110rpx auto 70rpx;
}
.img image{
    display:block;
    width:100%;
    height:100%;
}
.inf{
    display:block;
    font:30rpx/50rpx '微软雅黑';
    color:#333;
    text-align:center;
}
.tip{
    display:block;
    font:26rpx/40rpx '微软雅黑';
    color:#333;
    text-align:center;
}
